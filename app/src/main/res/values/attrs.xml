<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TwoStatesAVDImageView">
        <attr name="avd1" format="reference" />
        <attr name="avd2" format="reference" />
    </declare-styleable>
    <declare-styleable name="AVDImageView">
        <attr name="avd" format="reference" />
    </declare-styleable>
    <declare-styleable name="SwipeLayout">
        <attr name="swipeDirection" format="enum">
            <enum name="right" value="0" />
            <enum name="left" value="1" />
        </attr>
        <attr name="disableSwipe" format="boolean" />
        <attr name="swipeLimit" format="dimension" />
        <attr name="revertSwipeOnTouchUp" format="boolean" />
    </declare-styleable>
</resources>